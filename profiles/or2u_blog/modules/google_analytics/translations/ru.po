#: googleanalytics.admin.inc:93,116,89
msgid "Pages"
msgstr "Страницы"

#: googleanalytics.admin.inc:480,445
msgid "Value"
msgstr "Значение"

#: googleanalytics.admin.inc:479,444
msgid "Name"
msgstr "Название"

#: googleanalytics.admin.inc:14
msgid "General settings"
msgstr "Основные настройки"

#: googleanalytics.admin.inc:357,302
msgid "Advanced settings"
msgstr "Расширенные настройки"

#: googleanalytics.info:0
msgid "Statistics"
msgstr "Статистика"

#: googleanalytics.admin.inc:339,285
msgid "Page"
msgstr "Страница"

#: googleanalytics.admin.inc:407
msgid "Header"
msgstr "Заголовок"

#: googleanalytics.admin.inc:406
msgid "Footer"
msgstr "Подвал"

